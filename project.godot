[editor]

movie_writer/movie_file="C:/Users/User/Videos/godot_movie.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/ai_assistant_hub/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/dialogic/plugin.cfg")

[gui]

theme/custom="res://theme.tres"

