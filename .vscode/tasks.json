{
	"version": "2.0.0",
	"tasks": [

		{
            "label": "build",
            "command": "C:\\Users\\jesse\\OneDrive\\Desktop\\Godot_v4.6-dev3_mono_win64\\Godot_v4.6-dev3_mono_win64.exe",
            "type": "process",
            "args": [
                "--build-solutions",
                "--path",
                "${workspaceRoot}",
                "--no-window",
                "-q"
            ],
            "problemMatcher": "$msCompile"
        },
		{
			"label": "test",
			"command": "dotnet",
			"type": "process",
			"args": [
				"test",
				"--no-build",
				"--settings",
				".runsettings"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher": "$msCompile",
			"options": {
				"env": {
					"GODOT_BIN": "${env:GODOT_BIN}"
				}
			}
		},
	]
}
